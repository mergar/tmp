 -config.file=<%= scope['prometheus::alertmanager::config_dir'] -%>/<%= scope['prometheus::alertmanager::config_file'] -%>
<% if @dev -%>
 -dev=<%= scope['prometheus::alertmanager::dev'] -%>
<% end -%>
<% if @log_format -%>
 -log.format=<%= scope['prometheus::alertmanager::log_format'] -%>
<% end -%>
<% if @log_level -%>

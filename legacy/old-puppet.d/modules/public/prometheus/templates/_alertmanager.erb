<% end -%>
<% if @storage_path -%>
 -storage.path=<%= scope['prometheus::alertmanager::storage_path'] -%>
<% end -%>
<% if @web_external_url -%>
 -web.external-url=<%= scope['prometheus::alertmanager::web_external_url'] -%>
<% end -%>
